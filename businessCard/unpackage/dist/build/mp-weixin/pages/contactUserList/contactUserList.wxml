<view class="contact-container"><cu-custom vue-id="427fbaa8-1" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">搜索</view></cu-custom><view class="cu-bar bg-white search "><view class="search-form round"><text class="cuIcon-search"></text><input type="text" placeholder="输入搜索的关键词" confirm-type="search" data-event-opts="{{[['input',[['__set_model',['','inputV','$event',[]]],['searchChange',['$event']]]],['confirm',[['searchConfirm',['$event']]]]]}}" value="{{inputV}}" bindinput="__e" bindconfirm="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['searchConfirm',['$event']]]]]}}" class="cu-btn bg-gradual-green shadow-blur round" bindtap="__e">搜索</button></view></view><view class="tag-list"><block wx:for="{{tagList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><text data-event-opts="{{[['tap',[['tagClick',['$0',index],[[['tagList','id',item.id]]]]]]]}}" class="{{['item cu-tag round ',index===activeTagIndex?'active-tag bg-white':'']}}" bindtap="__e">{{''+item.title+''}}</text></block></view><view class="cu-list menu-avatar no-padding"><block wx:for="{{searchResList}}" wx:for-item="userItem" wx:for-index="sub" wx:key="sub"><view class="cu-item flex"><view class="cu-avatar self-avatar" style="width:2em;height:2em;"><image style="width:100%;height:100%;" src="{{userItem.avatar}}" mode="aspectFill"></image></view><view class="content"><view class="text-grey">{{userItem.name+' '+userItem.addInfo+''}}</view><view class="text-gray text-sm">{{''+userItem.phone+''}}</view></view></view></block></view></view>